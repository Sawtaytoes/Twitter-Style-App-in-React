{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "strict": [2, "never"],
    "no-console": 0,
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "^_*$",
      "varsIgnorePattern": "^_*$"
    }]
  },
  "plugins": [
    "graphql",
    "react"
  ]
}
